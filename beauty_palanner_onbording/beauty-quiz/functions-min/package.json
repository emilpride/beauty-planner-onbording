{
  "name": "beauty-quiz-functions-minimal",
  "version": "1.0.0",
  "private": true,
  "description": "Minimal Cloud Functions (finalizeOnboarding, generateAvatar) to avoid analyzer timeouts",
  "engines": { "node": "20" },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "firebase deploy --only functions:minimal",
    "clean": "node -e \"try{require('fs').rmSync('lib',{recursive:true,force:true})}catch(e){}\""
  },
  "dependencies": {
    "axios": "^1.7.7",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^5.1.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "typescript": "^5.6.3"
  }
}
